{
    "private": true,
    "scripts": {
        "dev-admin": "mix --mix-config=webpack.mix.admin.js",
        "dev-app": "mix",
        "dev": "concurrently \"yarn:dev-admin\" \"yarn:dev-app\"",
        "hot-admin": "mix watch --hot --mix-config=webpack.mix.admin.js",
        "hot-app": "mix watch --hot",
        "hot": "concurrently \"yarn:hot-admin\" \"yarn:hot-app\"",
        "prod-admin": "mix --production --mix-config=webpack.mix.admin.js",
        "prod-app": "mix --production",
        "prod": "concurrently \"yarn:prod-admin\" \"yarn:prod-app\"",
        "ssr-build": "mix --production --mix-config=webpack.mix.ssr.js",
        "ssr-start": "node public/assets/ssr/ssr.js",
        "ssr": "yarn prod-app && yarn ssr-build && yarn ssr-start",
        "preview-admin": "vite preview --mix-config=webpack.mix.admin.js",
        "preview-app": "vite preview",
        "preview": "concurrently \"yarn:preview-admin\" \"yarn:preview-app\""
    },
